$ mkdir /tmp/foobarbaz
$ cp data.txt /tmp/foobarbaz
$ cd /tmp/foobarbaz
$ xxd -r data.txt data.gz
$ file data.gz
$ gunzip data.gz
$ file data
$ mv data data.bz2
$ bunzip2 data.bz2
$ file data
$ mv data data.gz
$ gunzip data.gz
$ file data
$ mv data data.tar
$ tar -xf data.tar
$ file data5.bin
$ mv data5.bin data.tar
$ tar -xf data.tar
$ file data6.bin
$ mv data6.bin data.bz2
$ bunzip2 data.bz2
$ file data
$ mv data data.tar
$ tar -xf data.tar
$ file data8.bin
$ mv data8.bin data.gz
$ gunzip data.gz
$ file data
$ cat data
